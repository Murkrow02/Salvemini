<?xml version="1.0" encoding="UTF-8"?>
<ContentPage Title="App Info" xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="SalveminiApp.AreaVip.AppInfo">
    <ContentPage.Content>
        <StackLayout Spacing="1" Padding="10">
            <Entry IsEnabled="False" x:Name="version" Keyboard="Numeric" HorizontalOptions="FillAndExpand"/>
            <Entry IsEnabled="False" x:Name="orario" Keyboard="Numeric" HorizontalOptions="FillAndExpand"/>
            <Button Text="Aggiorna" Clicked="updateVersions"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
